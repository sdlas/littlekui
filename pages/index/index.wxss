@-webkit-keyframes load8 {     
  0% {     
    -webkit-transform: rotate(0deg);     
    transform: rotate(0deg);     
  }     
  100% {     
    -webkit-transform: rotate(360deg);     
    transform: rotate(360deg);     
  }     
}     
@keyframes load8 {     
  0% {     
    -webkit-transform: rotate(0deg);     
    transform: rotate(0deg);     
  }     
  100% {     
    -webkit-transform: rotate(360deg);     
    transform: rotate(360deg);     
  }     
}  







/*变大效果*/
@-webkit-keyframes ripple {
    70%{
    transform: scale(10);
    opacity: 0.5;
    }
    100% {
    transform: scale(1);
    opacity: 1;
    }
}

/*缩小效果，对应上*/

/*缩放效果*/
@-webkit-keyframes show {
    0%{
      transform: scale(0.1);
      opacity:0;
    }
    100% {
    transform: scale(1);
    opacity: 1;
    }
}

/*向右移出视野*/
@-webkit-keyframes turnleft {
    100% {
      transform: translateX(600px);
    }
}

/*对话框的缩放*/
@-webkit-keyframes chat {
    0%{
      transform:scale(1);
    }
    50%{
      transform:scale(1.1);
    }
    100% {
      transform:scale(1);
    }
}